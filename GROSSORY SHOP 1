#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main()
{
    int A4sheet,Pen,Pencil,Eraser;
    scanf("%d\n",&A4sheet);
    scanf("%d\n",&Pen);
    scanf("%d\n",&Pencil);
    scanf("%d\n",&Eraser);
    printf("Items Details\n");
    printf("A4sheet:%d\n",A4sheet);
    printf("Pen:%d\n",Pen);
    printf("Pencil:%d\n",Pencil);
    printf("Eraser:%d",Eraser);
    return 0;
}

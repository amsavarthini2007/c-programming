#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main()
{
    int a,count=0;
    scanf("%d",&a);
    if(a>=1 && a<=10000000)
    {
        int t=a;
        while(t>0)
        {
            t/=10;
            count++;
        }
        printf("The count of the given integer is: %d\n",count);
    }
    else
    {
        printf("Enter a Valid Input\n");
    }

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
